{
  "env": {
    "browser": false,
    "mocha": false,
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "extends": [ "airbnb/base", "prettier"],
  "rules": {
    "comma-dangle": ["error", "never"],
    "semi": 2,
    "indent": ["error", 2],
    "no-trailing-spaces": 1,
    "no-console": 1,
    "class-methods-use-this": 0,
    "no-param-reassign" : 0
   }
}
